<csw:GetRecordsResponse xmlns:py="http://genshi.edgewall.org/" xsi:schemaLocation="http://www.opengis.net/cat/csw/2.0.2 http://schemas.opengis.net/csw/2.0.2/CSW-discovery.xsd http://www.isotc211.org/2005/gmd http://schemas.opengis.net/iso/19139/20070417/gmd/gmd.xsd" xmlns:csw="http://www.opengis.net/cat/csw/2.0.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0.2" xmlns:xi="http://www.w3.org/2001/XInclude">

  <?python from genshi import XML ?>

  <csw:SearchStatus timestamp="${timestamp}"/>
  <csw:SearchResults elementSet="full" recordSchema="http://www.opengis.net/cat/csw/2.0.2" numberOfRecordsMatched="${results['matches']}" numberOfRecordsReturned="${results['returned']}" nextRecord="${results['next']}" expires="${timestamp}">

    <py:for each="record in results['records']">
    ${XML(record)}
    </py:for>
  </csw:SearchResults>
</csw:GetRecordsResponse>
